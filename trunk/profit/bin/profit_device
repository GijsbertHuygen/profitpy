import sys
from PyQt4.QtGui import QApplication
from profit.main import MainWindow
from profit.lib.core import Settings

app = QApplication(sys.argv)
app.setApplicationName(Settings.keys.app)
window = MainWindow()
window.show()
sys.exit(app.exec_())

