import sys
from PyQt4.QtGui import QApplication
from profit.lib.core import Settings
from profit.widgets.profitdevice import ProfitDeviceWindow

app = QApplication(sys.argv)
app.setApplicationName(Settings.keys.app)
window = ProfitDeviceWindow()
window.show()
sys.exit(app.exec_())

